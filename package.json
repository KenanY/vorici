{
  "name": "vorici",
  "license": "MIT",
  "private": true,
  "scripts": {
    "bundle": "npm run css && npm run js && npm run html",
    "html": "sed -r 's/index.js/bundle.js/g' < index.html | html-inline > bundle.html",
    "js": "browserify -g uglifyify -t babelify -p bundle-collapser/plugin index.js | uglifyjs -cm > bundle.js",
    "css": "postcss -u postcss-cssnext style.css | cssnano > bundle.css",
    "dev": "npm run css && budo index.js --live -- -t babelify",
    "size": "npm run js && gzip -9 < bundle.js | wc -c | pretty-bytes",
    "lint": "eslint index.js"
  },
  "dependencies": {
    "chromatic-orb": "^1.1.0",
    "geometric-variance": "^1.0.0",
    "global": "^4.3.0",
    "griddle-react": "^0.7.0",
    "lodash.assign": "^4.0.0",
    "number-grouper": "^0.0.1",
    "react": "^15.0.0",
    "react-dom": "^15.0.0",
    "round-to": "^1.1.0",
    "socket-colors-chance": "^1.0.0",
    "underscore": "^1.8.3"
  },
  "devDependencies": {
    "@kenan/eslint-config": "^4.0.0",
    "babel-plugin-check-es2015-constants": "^6.8.0",
    "babel-plugin-syntax-jsx": "^6.18.0",
    "babel-plugin-transform-es2015-arrow-functions": "^6.8.0",
    "babel-plugin-transform-es2015-block-scoping": "^6.18.0",
    "babel-plugin-transform-es2015-classes": "^6.18.0",
    "babel-plugin-transform-es2015-function-name": "^6.9.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.18.0",
    "babel-plugin-transform-es2015-template-literals": "^6.8.0",
    "babel-plugin-transform-react-jsx": "^6.8.0",
    "babelify": "^7.2.0",
    "browserify": "^13.0.0",
    "budo": "^9.1.0",
    "bundle-collapser": "^1.2.1",
    "cssnano-cli": "^1.0.3",
    "eslint": "^3.9.1",
    "eslint-plugin-react": "^6.5.0",
    "html-inline": "^1.2.0",
    "postcss-cli": "^2.3.3",
    "postcss-cssnext": "^2.8.0",
    "pretty-bytes-cli": "^2.0.0",
    "uglify-js": "^2.6.1",
    "uglifyify": "^3.0.1"
  }
}
